    "description": "Laravel 5.4 notifications using toastr",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "toastr", "notification"],
    "authors": [
        {
            "name": "Hugo Rodrigues",
            "email": "hrodrigues1984@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
